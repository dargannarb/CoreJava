	// Interface --> it is like a class
	// abstract
	
	/*
	 * 
	Mobile
	
	types - oneplus -- 7,7t,8,8t,9,9r,9pro 120 displ os 
	
	
	
	*/
	
	public static void main(String[] args) {
		
		Animals A= new Animals();
		A.breath();
		A.eat();
		A.walk();
		A.sense();
		A.think();
		
		
		Humans H=new Humans(); 
		H.breath();
		H.eat();
		H.walk();
		H.sense();

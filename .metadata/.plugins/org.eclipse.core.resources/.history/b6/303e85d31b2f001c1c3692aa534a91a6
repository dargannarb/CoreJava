package Basics;

import java.util.Arrays;

public class Test {
	// is main method to identify by compiler
	public static void main(String[] args) {
	
		int [] a= {3,4,5,5};// for each
		
		for(int x:a) {
			System.out.println(x);
		}
		
		int [] b=new int[7];//100 // 1000 // 
		b[0]=3;
		b[1]=4;
		b[2]=5;

		//System.out.println(b);
		//System.out.println(Arrays.toString(b));
		
		int [][] c=new int[4][];// 0-4 , 1- 2,  2 -3 ,3-6
		c[0]=new int[4];
		c[1]=new int [2];
		c[2]=new int[3];
		c[3]=new int[6];
		
		c[0][0]=11;
		c[0][1]=12;
		c[0][2]=13;
		c[0][3]=14;
		
		
		c[1][0]=21;
		c[1][1]=22;
		

		c[2][0]=31;
		c[2][1]=32;
		c[2][2]=33;
		
		c[3][0]=41;
		c[3][1]=42;
		c[3][2]=43;
		c[3][3]=44;
		c[3][4]=45;
		c[3][5]=46;
	
		
		int [][][] arr=new int [2][2][2];
		
		arr[0][0][0]=19;
		arr[0][0][1]=18;
		
		arr[0][1][0]=29;
		arr[0][1][1]=28;
		
		
		arr[1][0][0]=19;
		arr[1][0][1]=18;
		
		arr[1][1][0]=29;
		arr[1][1][1]=28;
		
		
		
		
		for(int[][] x:arr) {// row []
			for(int y[]:x) {// cols
				System.out.println(y);
			}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		String []str=new String[5];
		String []str1= {"A","B"};
		str[0]="A";
		str[1]="B";
		//sSystem.out.println(Arrays.toString(str));
		
	}
	

}
